<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Automation Test Suite" parallel="tests" thread-count="1">
	
	<listeners>
		<listener
			class-name="com.openmrs.listeners.AnnotationTransformer" />
		<listener class-name="com.openmrs.listeners.ListenerClass" />
	</listeners>


    <test name="Regression Tests execution over Firefox Browser"  parallel="methods">
	   <parameter name="browser" value="CHROME" />
	    <classes>
	    	<class name="com.openmrs.tests.OpenMRsTest"></class>
	    </classes>    
	</test>
	   
	   
</suite>
